program StructParameterTest {
    struct Point {
        int x;
        int y;
    }
    
    struct Circle : Point {
        double radius;
    }
    
    double calculateArea(Circle c) {
        return 3.14159 * c.radius * c.radius;
    }
    
    int main() {
        Circle circle;
        Point point;
        
        calculateArea(circle);     // صحيح
        calculateArea(point);      // خطأ: نوع غير متوافق
        calculateArea(null);       // صحيح (null مقبول للهياكل)
        
        return 0;
    }
}